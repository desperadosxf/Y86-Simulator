def	GetIndex(temp):
	l=0
	r=len(LineNum)-1
	mid=(l+r)/2
	while (LineNum[mid]!=temp):
		if(LineNum[mid]<temp):
			l=mid+1
		else:
			r=mid
		mid=(l+r)/2;
	
	while True:
		if(mid+1==len(LineNum)):
			break
		if(LineNum[mid+1]==LineNum[mid]):
			mid+=1
		else:
			break

	return mid


